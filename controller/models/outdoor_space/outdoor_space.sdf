<?xml version="1.0"?>
<sdf version="1.5">
 <model name="outdoor_space">
   <static>true</static>
   <link name="outdoor_surf">
    <collision name="outdoor_collision">
    <!-- <geometry>
       <box>
        <size>40 40 0.01</size>
       </box>
     </geometry> -->
     <geometry>
       <mesh>
         <uri>model://outdoor_space/meshes/ground.dae</uri>
       </mesh>
     </geometry>

    </collision>

     <visual name="surface_mesh">
      <cast_shadows>true</cast_shadows>
      <geometry>
       <mesh>
         <uri>model://outdoor/meshes/ground.dae</uri>
       </mesh>
      </geometry>
      <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Wood</name>
            </script>
          </material>
     </visual>
   </link>

<static>true</static>

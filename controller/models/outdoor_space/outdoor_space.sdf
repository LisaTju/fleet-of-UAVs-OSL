       <mesh>
         <uri>model://outdoor_space/meshes/building1.dae</uri>
       </mesh>
      </geometry>
      <material>
        <script> 
            <uri>model://outdoor_space/materials/scripts</uri>
          <uri>model://outdoor_space/materials/textures</uri>
          <name>outdoor_space/building</name> 
        </script>
      </material>
     </visual>
   </link>


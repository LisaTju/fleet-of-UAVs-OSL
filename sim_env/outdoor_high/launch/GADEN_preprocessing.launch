<!-- Mission file for GADEN preprocessing
    ROS - KINETIC
-->

<launch>
    <arg name="scenario" default="outdoor_high" />
    
    <node pkg="gaden_preprocessing" type="preprocessing" name="preprocessing" output="screen">
     
        #Cell size
        <param name="cell_size" value="1"/>    
        
        #CAD models of the enviroment (.stl)
        <param name="number_of_models" value="12"/>
        <param name="model_0" value="$(find test_env)/$(arg scenario)/cad_models/Hrbuilding1.stl"/>
        <param name="model_1" value="$(find test_env)/$(arg scenario)/cad_models/Hrbuilding2.stl"/>
        <param name="model_2" value="$(find test_env)/$(arg scenario)/cad_models/Hrbuilding3.stl"/>
        <param name="model_3" value="$(find test_env)/$(arg scenario)/cad_models/Hrbuilding4.stl"/>
        <param name="model_4" value="$(find test_env)/$(arg scenario)/cad_models/Hrbuilding5.stl"/>
        <param name="model_5" value="$(find test_env)/$(arg scenario)/cad_models/Hrbuilding6.stl"/>
        <param name="model_6" value="$(find test_env)/$(arg scenario)/cad_models/Hrbuilding7.stl"/>
        <param name="model_7" value="$(find test_env)/$(arg scenario)/cad_models/Hrbuilding8.stl"/>
        <param name="model_8" value="$(find test_env)/$(arg scenario)/cad_models/Hrbuilding9.stl"/>
        <param name="model_9" value="$(find test_env)/$(arg scenario)/cad_models/inlet.stl"/>
        <param name="model_10" value="$(find test_env)/$(arg scenario)/cad_models/wall2.stl"/>
        <param name="model_11" value="$(find test_env)/$(arg scenario)/cad_models/wall1.stl"/>
                
        #CAD model of the outlets (.stl)
        <param name="number_of_outlet_models" value="1"/>
        <param name="outlets_model_0" value="$(find test_env)/$(arg scenario)/cad_models/outlet.stl"/>
        
        #3D Location of a point in free-space
        <param name="empty_point_x" value="1.0"/>      ### (m)
        <param name="empty_point_y" value="1.0"/>      ### (m)
        <param name="empty_point_z" value="0.5"/>      ### (m)
            
        #Wind Data (the node will append _i.csv to the name that is specified here)
       <!--<param name="wind_files" value="$(find test_env)/$(arg scenario)/open_data15/wind_cell"/> 
               <param name="wind_files" value="$(find test_env)/$(arg scenario)/winddata15s/wind_cell_cent"/>
         <param name="wind_files" value="$(find test_env)/$(arg scenario)/wind/wind_outside"/>
         <param name="wind_files" value="$(find test_env)/$(arg scenario)/newwinddata/varwindnew"/>-->
          <param name="wind_files" value="$(find test_env)/$(arg scenario)/data_hr/openrisedata"/>

        #Where to write the output files
        <param name="output_path" value="$(find test_env)/$(arg scenario)" />
    </node>
</launch>
